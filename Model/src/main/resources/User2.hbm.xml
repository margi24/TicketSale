<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.User2"
           table="User2">
        <id name="id" column="UserId">
            <generator class="increment"/>
        </id>
        <property
                name="nume"
                column="Nume"/>
        <property
                name="parola"
                column="Parola"/>
    </class>
</hibernate-mapping>